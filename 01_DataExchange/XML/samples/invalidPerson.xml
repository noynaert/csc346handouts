<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE person [
    <!ELEMENT person (name,email,phone)>
    <!ELEMENT name (#PCDATA)>
    <!ELEMENT email (#PCDATA)>
    <!ELEMENT phone (#PCDATA)>
    <!ATTLIST phone type CDATA "cell|work|fax|home">

]>
<person>
    <name>John Doe</name>
    <phone type="cell">555-555-5555</phone>
</person>